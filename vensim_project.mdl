Annual Growth Rate=0.05
	~	1/Year
	~		|

Annual Health Expenditure=Cost Per Microgram*"PM2.5 Concentration"
	~	INR/Year
	~		|

Average Emission Per Vehicle = WITH LOOKUP (Time,
		( [(2025,0)-(2040,0.1)], (2025,0.044), (2028,0.038), (2030,0.028), (2035,0.015), (2040\
		,0.008) ) )
	~	g/kg
	~		|

Average Vehicle Lifespan=12
	~	Year
	~		|

Baseline Dissipation=0.9
	~	Dmnl
	~		|

Cost Per Microgram=5e+07
	~	
	~		|

Cumulative Health Cost= INTEG (Annual Health Expenditure,
		0)
	~	
	~		~	:SUPPLEMENTARY 
	|

EV Adoption Fraction=IF THEN ELSE( Policy Switch = 1, EV Lookup Aggressive(Time), EV Lookup BAU\
		(Time) )
	~	Dmnl
	~		|

EV Lookup Aggressive([(2025,0)-(2040,1)],(2025,0.2), (2027,0.95), (2030,0.98), (2040,\
		0.95))
	~	Dmnl
	~		|

EV Lookup BAU([(2025,0)-(2040,1)],(2025,0.02), (2030,0.1), (2035,0.18), (2040,0.25))
	~	Dmnl
	~		|

Natual Dissipitation fraction=Baseline Dissipation + (Seasonal Amplitude * SIN( 2 * 3.14159\
		 * Time / 1 ))
	~	1/Year
	~		|

"PM2.5 Concentration"= INTEG (Pollution Generation Rate-Pollution Dissipitation+"PM2.5 Concentration"\
		,
		101)
	~	ug/(m*m*m)
	~		|

Policy Switch=1
	~	Dmnl
	~		|

Pollution Dissipitation="PM2.5 Concentration"*Natual Dissipitation fraction
	~	ug/m3/Year
	~		|

Pollution Generation Rate=(Total Vehicles * (1 - EV Adoption Fraction) * Average Emission Per Vehicle\
		 * 12000) / 1e+06
	~	Tons/Year
	~		|

Seasonal Amplitude=0.3
	~	Dmnl
	~		|

Total Vehicles= INTEG (Vehicle Purchase Rate-Vehicle Scrappage Rate,
		1.43e+07)
	~	Vehicle
	~		|

Vehicle Purchase Rate=Annual Growth Rate*Total Vehicles
	~	Vehicles/Year
	~		|

Vehicle Scrappage Rate=IF THEN ELSE( Total Vehicles > 0, Total Vehicles / Average Vehicle Lifespan\
		, 0 )
	~	
	~		|

